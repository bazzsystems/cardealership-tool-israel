<Window x:Class="WpfApp1.PasswordEntryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Bazz Systems - Enter Password" Height="200" Width="400">
    <StackPanel Margin="10">
        <Label Content="Password 1" />
        <PasswordBox Name="PasswordBox1" />
        <Label Content="Password 2" />
        <PasswordBox Name="PasswordBox2" />
        <Button Content="Submit" Click="SubmitButton_Click" Margin="0,10,0,0" />
    </StackPanel>
</Window>
